{
  "name": "lerna-taro-fast-framework",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "kityandhero <kityandhero@126.com>",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:all": "npm run build:core-common-compression && npm run build:core-component-compression && npm run build:core-component-extra-compression && npm run build:core-component-prism-compression && npm run build:core-framework-compression",
    "build:core-common-compression": "cd packages/taro-fast-common && npm run build",
    "build:core-common-skip-compression": "cd packages/taro-fast-common && npm run build:skipCompression",
    "build:core-component-compression": "cd packages/taro-fast-component && npm run build",
    "build:core-component-extra-compression": "cd packages/taro-fast-component-extra && npm run build",
    "build:core-component-extra-skip-compression": "cd packages/taro-fast-component-extra && npm run build:skipCompression",
    "build:core-component-prism-compression": "cd packages/taro-fast-component-prism && npm run build",
    "build:core-component-prism-skip-compression": "cd packages/taro-fast-component-prism && npm run build:skipCompression",
    "build:core-component-skip-compression": "cd packages/taro-fast-component && npm run build:skipCompression",
    "build:core-framework-compression": "cd packages/taro-fast-framework && npm run build",
    "build:core-framework-skip-compression": "cd packages/taro-fast-framework && npm run build:skipCompression",
    "build:core:simple:h5": "npm run build:all && npm run build:simple:h5",
    "build:core:simple:weapp": "npm run build:all && npm run build:simple:weapp",
    "build:simple:h5": "cd packages/simple-taro-fast-framework && npm run build:h5",
    "build:simple:weapp": "cd packages/simple-taro-fast-framework && npm run build:weapp",
    "change-nrm-local": "echo change nrm use npm && nrm use npm && npx sleepms 2000",
    "change-nrm-npm": "echo change nrm use npm && nrm use npm && npx sleepms 2000",
    "changelog": "lerna version --conventional-commits --no-push --no-git-tag-version",
    "preclean:all": "rimraf ./yarn-error.log && rimraf ./yarn.lock",
    "clean:all": "lerna clean -y",
    "postclean:all": "rimraf ./package-lock.json && npm run clean:core-cli && npm run clean:core-common && npm run clean:core-component-extra && npm run clean:core-component-prism && npm run clean:core-framework && npm run clean:core-rollup && npm run clean:simple && rimraf ./node_modules",
    "clean:core-cli": "cd packages/taro-fast-cli && npm run clean-file",
    "clean:core-common": "cd packages/taro-fast-common && npm run clean-file",
    "clean:core-component": "cd packages/taro-fast-component && npm run clean-file",
    "clean:core-component-extra": "cd packages/taro-fast-component-extra && npm run clean-file",
    "clean:core-component-prism": "cd packages/taro-fast-component-prism && npm run clean-file",
    "clean:core-framework": "cd packages/taro-fast-framework && npm run clean-file",
    "clean:core-rollup": "cd packages/taro-fast-rollup && npm run clean-file",
    "clean:simple": "cd packages/simple-taro-fast-framework && npm run clean-file",
    "commitlint": "npx commitlint --edit",
    "precz": "npm run update:cz:flag && git stage -A",
    "cz": "cz",
    "postcz": "git push",
    "dev:core-common": "cd packages/taro-fast-common && npm run dev",
    "dev:core-component": "cd packages/taro-fast-component && npm run dev",
    "dev:core-component-extra": "cd packages/taro-fast-component-extra && npm run dev",
    "dev:core-framework": "cd packages/taro-fast-framework && npm run dev",
    "lerna": "lerna",
    "lerna:b": "husky install && git pull && pnpm install",
    "lerna:build": "lerna run --stream --sort build",
    "lerna:publish": "lerna publish --yes",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "npx eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:all:fix": "npm run lint:core-common:fix && npm run lint:core-component:fix && npm run lint:core-component-extra:fix && npm run lint:core-component-prism:fix && npm run lint:core-framework:fix && npm run lint:simple:fix",
    "lint:core-common:fix": "cd packages/taro-fast-common && ncu run lint:fix",
    "lint:core-component-extra:fix": "cd packages/taro-fast-component-extra && ncu run lint:fix",
    "lint:core-component-prism:fix": "cd packages/taro-fast-component-prism && ncu run lint:fix",
    "lint:core-component:fix": "cd packages/taro-fast-component && ncu run lint:fix",
    "lint:core-framework:fix": "cd packages/taro-fast-framework && ncu run lint:fix",
    "lint:js": "npx eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "npx prettier --check \"**/*\" --end-of-line auto",
    "lint:simple:fix": "cd packages/simple-taro-fast-framework && ncu run lint:fix",
    "lint:style": "npx stylelint --fix \"src/**/*.less\"",
    "ncu:all": "ncu && npm run ncu:core-rollup && npm run ncu:core-cli && npm run ncu:core-common && npm run ncu:core-component && npm run ncu:core-component-extra && npm run ncu:core-component-prism && npm run ncu:core-framework && npm run ncu:simple",
    "ncu:core-cli": "cd packages/taro-fast-cli && ncu",
    "ncu:core-common": "cd packages/taro-fast-common && ncu",
    "ncu:core-component": "cd packages/taro-fast-component && ncu",
    "ncu:core-component-extra": "cd packages/taro-fast-component-extra && ncu",
    "ncu:core-component-prism": "cd packages/taro-fast-component-prism && ncu",
    "ncu:core-framework": "cd packages/taro-fast-framework && ncu",
    "ncu:core-rollup": "cd packages/taro-fast-rollup && ncu",
    "ncu:simple": "cd packages/simple-taro-fast-framework && ncu",
    "ncu:u:all": "ncu -u && npm run ncu:u:core-rollup && npm run ncu:u:cli && npm run ncu:u:core-common && npm run ncu:u:core-component && npm run ncu:u:core-component-extra && npm run ncu:u:core-component-prism && npm run ncu:u:core-framework && npm run ncu:u:simple",
    "ncu:u:cli": "cd packages/taro-fast-common && ncu -u",
    "ncu:u:core-common": "cd packages/taro-fast-common && ncu -u",
    "ncu:u:core-component": "cd packages/taro-fast-component && ncu -u",
    "ncu:u:core-component-extra": "cd packages/taro-fast-component-extra && ncu -u",
    "ncu:u:core-component-prism": "cd packages/taro-fast-component-prism && ncu -u",
    "ncu:u:core-framework": "cd packages/taro-fast-framework && ncu -u",
    "ncu:u:core-rollup": "cd packages/taro-fast-rollup && ncu -u",
    "ncu:u:simple": "cd packages/simple-taro-fast-framework && ncu -u",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "prettier:all": "npm run prettier:core-common && npm run prettier:core-component && npm run prettier:core-component-extra && npm run prettier:core-component-prism && npm run prettier:core-framework && npm run prettier:other-simple",
    "prettier:core-common": "cd packages/taro-fast-common && npm run prettier",
    "prettier:core-component": "cd packages/taro-fast-component && npm run prettier",
    "prettier:core-component-extra": "cd packages/taro-fast-component-extra && npm run prettier",
    "prettier:core-component-prism": "cd packages/taro-fast-component-prism && npm run prettier",
    "prettier:core-framework": "cd packages/taro-fast-framework && npm run prettier",
    "prettier:other-simple": "cd packages/simple-taro-fast-framework && npm run prettier",
    "prepublish-npm-all": "npm run change-nrm-npm",
    "publish-npm-all": "nrm ls && npm run publish:npm-all && nrm use local && npx sleepms 2000",
    "postpublish-npm-all": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-cli": "npm run change-nrm-npm",
    "publish-npm-cli": "nrm ls && npm run publish:npm-cli && nrm use local && npx sleepms 2000",
    "postpublish-npm-cli": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-common": "npm run change-nrm-npm",
    "publish-npm-common": "nrm ls && npm run publish:npm-common && nrm use local && npx sleepms 2000",
    "postpublish-npm-common": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-common-extra": "npm run change-nrm-npm",
    "publish-npm-common-extra": "nrm ls && npm run publish:npm-common-extra && nrm use local && npx sleepms 2000",
    "postpublish-npm-common-extra": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-common-prism": "npm run change-nrm-npm",
    "publish-npm-common-prism": "nrm ls && npm run publish:npm-common-prism && nrm use local && npx sleepms 2000",
    "postpublish-npm-common-prism": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-framework": "npm run change-nrm-npm",
    "publish-npm-framework": "nrm ls && npm run publish:npm-framework && nrm use local && npx sleepms 2000",
    "postpublish-npm-framework": "npm run change-nrm-local && nrm ls",
    "prepublish:build": "npm run cz && npm run change-nrm-local && npm run build:all",
    "publish:build": "npm run publish:lerna",
    "postpublish:build": "npm run publish-npm-all",
    "publish:lerna": "lerna updated && lerna publish --yes",
    "publish:npm-all": "nrm use npm && npx sleepms 2000 && nrm ls && npm run publish:npm-common-only && npm run publish:npm-component-only && npm run publish:npm-component-extra-only && npm run publish:npm-component-prism-only && npm run publish:npm-framework-only && npm run publish:npm-cli-only && nrm use local && echo sleep 2000ms && npx sleepms 2000 && nrm ls",
    "publish:npm-cli": "cd packages/taro-fast-cli && npm publish",
    "publish:npm-common": "cd packages/taro-fast-common && npm publish",
    "publish:npm-component": "nrm use npm && nrm ls && npm run publish:npm-component-only && nrm use local && nrm ls",
    "publish:npm-component-extra": "cd packages/taro-fast-component-extra && npm publish",
    "publish:npm-component-prism": "cd packages/taro-fast-component-prism && npm publish",
    "publish:npm-framework": "cd packages/taro-fast-framework && npm publish",
    "publish:v": "npm run build:all && lerna updated && lerna publish --yes",
    "reinstall": "npm run clean:all && npm run lerna:b",
    "show:all": "lerna ls -a -l",
    "start": "npm run start:simple:weapp",
    "start:simple:h5": "cd packages/simple-taro-fast-framework && npm run start:h5",
    "start:simple:weapp": "cd packages/simple-taro-fast-framework && npm run start",
    "update:cz:flag": "node cz.flag.refresh.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.7",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/config-lerna-scopes": "^17.4.2",
    "@commitlint/cz-commitlint": "^17.4.2",
    "commitizen": "^4.2.6",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-taro": "^3.5.11",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.3",
    "lerna": "^6.4.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3",
    "prettier-plugin-packagejson": "^2",
    "rollup-plugin-copy": "^3.4.0",
    "sleep-ms": "^2.0.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0"
  }
}
