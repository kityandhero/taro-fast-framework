{
  "name": "lerna-taro-fast-framework",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "adjust-install:framework": "cd packages/taro-fast-framework && npm run adjust-install",
    "adjust-install:simple": "cd packages/simple-taro-fast-framework && npm run adjust-install",
    "lerna:b": "npm run clean && husky install && git pull && lerna bootstrap",
    "lerna:create": "lerna create",
    "lerna:build": "lerna run --stream --sort build",
    "lerna:publish": "lerna publish --yes",
    "changelog": "lerna version --conventional-commits --no-push --no-git-tag-version",
    "lerna": "lerna",
    "clean": "rimraf ./yarn-error.log && rimraf ./yarn.lock && rimraf ./package-lock.json && lerna clean -y",
    "publish:f": "npm run build:f-compression && npm run scz && lerna updated && lerna publish --yes",
    "publish:c": "npm run scz && lerna updated && lerna publish --yes",
    "publish:v": "lerna updated && lerna publish --yes",
    "publish:npm": "cd packages/taro-fast-framework && nrm use npm && nrm ls && npm publish && nrm use vd && nrm ls",
    "cz": "cz",
    "scz": "git stage -A && npm run cz && git push",
    "commitlint": "npx commitlint --edit",
    "lint-staged": "lint-staged",
    "lint:f": "cd packages/taro-fast-framework && npm run lint",
    "build:fvp": "npm run build:f && npm run build:vp",
    "build:f-noCompression": "cd packages/taro-fast-framework && npm run build:rollup-noCompression",
    "build:f-compression": "cd packages/taro-fast-framework && npm run build:rollup-compression",
    "build:p": "cd packages/taro-fast-framework && npm publish",
    "build:vp": "cd packages/taro-fast-framework && npm version patch && npm publish",
    "build:fp": "cd packages/taro-fast-framework && npm publish",
    "build:start": "npm run build:f && npm start",
    "clean:framework": "cd packages/taro-fast-framework && npm run clean-file",
    "clean:simple": "cd packages/simple-taro-fast-framework && npm run clean-file",
    "reinstall": "npm run clean && npm run clean:framework && npm run clean:simple && npm run lerna:b",
    "show:v": "cd packages/taro-fast-framework && npm version",
    "start:simple": "cd packages/simple-taro-fast-framework && npm start",
    "start:build": "npm run build:f-noCompression && npm run start:simple",
    "start": "npm run start:simple",
    "build:simple": "cd packages/simple-taro-fast-framework && npm run build:weapp",
    "build": "npm run build:f-compression && npm run build:simple",
    "ncu:u:all": "ncu -u && npm run ncu:u:framework && npm run ncu:u:simple",
    "ncu:u:framework": "cd packages/taro-fast-framework && ncu -u",
    "ncu:u:simple": "cd packages/simple-taro-fast-framework && ncu -u",
    "ncu:all": "ncu && npm run ncu:framework && npm run ncu:simple",
    "ncu:framework": "cd packages/taro-fast-framework && ncu",
    "ncu:simple": "cd packages/simple-taro-fast-framework && ncu"
  },
  "dependencies": {
    "rollup-plugin-copy": "^3.4.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/config-lerna-scopes": "^16.0.0",
    "@commitlint/cz-commitlint": "^16.0.0",
    "commitizen": "^4.2.4",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "lint-staged": "^12.1.4",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "main": "index.js",
  "author": "kityandhero <kityandhero@126.com>",
  "license": "MIT"
}
