{
  "name": "lerna-taro-fast-framework",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "kityandhero <kityandhero@126.com>",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:all": "npm run build:core-common-compression && npm run build:core-component-compression && npm run build:core-component-extra-compression && npm run build:core-component-prism-compression && npm run build:core-framework-compression",
    "build:core-common-compression": "cd packages/taro-fast-common && npm run build",
    "build:core-common-skip-compression": "cd packages/taro-fast-common && npm run build:skipCompression",
    "build:core-component-compression": "cd packages/taro-fast-component && npm run build",
    "build:core-component-extra-compression": "cd packages/taro-fast-component-extra && npm run build",
    "build:core-component-extra-skip-compression": "cd packages/taro-fast-component-extra && npm run build:skipCompression",
    "build:core-component-prism-compression": "cd packages/taro-fast-component-prism && npm run build",
    "build:core-component-prism-skip-compression": "cd packages/taro-fast-component-prism && npm run build:skipCompression",
    "build:core-component-skip-compression": "cd packages/taro-fast-component && npm run build:skipCompression",
    "build:core-framework-compression": "cd packages/taro-fast-framework && npm run build",
    "build:core-framework-skip-compression": "cd packages/taro-fast-framework && npm run build:skipCompression",
    "build:simple:h5": "cd packages/simple-taro-fast-framework && npm run build:h5",
    "build:simple:weapp": "cd packages/simple-taro-fast-framework && npm run build:weapp",
    "change-nrm-local": "nrm use local && npm run sleep:change-nrm",
    "change-nrm-npm": "nrm use npm && npm run sleep:change-nrm",
    "changelog": "lerna version --conventional-commits --no-push --no-git-tag-version",
    "clean:all": "node ./develop/assists/clean.js",
    "commitlint": "npx commitlint --edit",
    "config:env": "node ./develop/assists/env.init.js",
    "postconfig:env": "npm run prettier:package.json:all",
    "precz": "npm run update:cz:flag && git stage -A",
    "cz": "cz",
    "postcz": "git push",
    "dev:core-common": "cd packages/taro-fast-common && npm run dev:rollup",
    "dev:core-component": "cd packages/taro-fast-component && npm run dev:rollup",
    "dev:core-component-extra": "cd packages/taro-fast-component-extra && npm run dev:rollup",
    "dev:core-component-prism": "cd packages/taro-fast-component-prism && npm run dev:rollup",
    "dev:core-framework": "cd packages/taro-fast-framework && npm run dev:rollup",
    "postinstall": "npm run config:env",
    "install:dependence:dev": "node ./develop/assists/globalDependence.js",
    "postinstall:dependence:dev": "pnpm install",
    "prelerna:bootstrap": "npm run change-nrm-local",
    "lerna:bootstrap": "npm run clean:all && husky install && git pull && pnpm install",
    "lerna:publish": "lerna publish --yes",
    "lint-staged": "npx lint-staged",
    "lint:staged": "npx lint-staged",
    "prencu:all": "npm run change-nrm-local",
    "ncu:all": "node ./develop/assists/ncu.all.js",
    "prencu:u:all": "npm run change-nrm-local",
    "ncu:u:all": "node ./develop/assists/ncu.u.all.js",
    "postncu:u:all": "pnpm install",
    "ncu:u:special": "node ./develop/assists/ncu.u.special.js",
    "postncu:u:special": "pnpm install",
    "precommit": "npm run lint:staged",
    "prepare": "husky install",
    "prettier:format:all": "npx prettier --write .",
    "prettier:format:change": "npx prettier --cache --write .",
    "prettier:package.json:all": "npx prettier --write ./**/package.json",
    "prettier:package.json:current": "npx prettier --write ./package.json",
    "prepublish-npm-all": "npm run change-nrm-npm",
    "publish-npm-all": "nrm ls && npm run publish:npm-all",
    "postpublish-npm-all": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-cli": "npm run change-nrm-npm",
    "publish-npm-cli": "nrm ls && npm run publish:npm-cli",
    "postpublish-npm-cli": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-common": "npm run change-nrm-npm",
    "publish-npm-common": "nrm ls && npm run publish:npm-common",
    "postpublish-npm-common": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-common-extra": "npm run change-nrm-npm",
    "publish-npm-common-extra": "nrm ls && npm run publish:npm-common-extra",
    "postpublish-npm-common-extra": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-common-prism": "npm run change-nrm-npm",
    "publish-npm-common-prism": "nrm ls && npm run publish:npm-common-prism",
    "postpublish-npm-common-prism": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-framework": "npm run change-nrm-npm",
    "publish-npm-framework": "nrm ls && npm run publish:npm-framework",
    "postpublish-npm-framework": "npm run change-nrm-local && nrm ls",
    "prepublish:build": "pnpm install && npm run cz && npm run build:all",
    "publish:build": "npm run publish:lerna",
    "postpublish:build": "npm run change-nrm-local",
    "prepublish:lerna": "npm run change-nrm-npm",
    "publish:lerna": "lerna updated && npm run lerna:publish",
    "postpublish:lerna": "npm run change-nrm-local && npm run publish-npm-all",
    "publish:npm-all": "npm run change-nrm-npm && npm run publish:npm-common && npm run publish:npm-component && npm run publish:npm-component-extra && npm run publish:npm-component-prism && npm run publish:npm-framework && npm run publish:npm-cli && npm run change-nrm-local && nrm ls",
    "publish:npm-cli": "cd packages/taro-fast-cli && npm publish",
    "publish:npm-common": "cd packages/taro-fast-common && npm publish",
    "publish:npm-component": "cd packages/taro-fast-component && npm publish",
    "publish:npm-component-extra": "cd packages/taro-fast-component-extra && npm publish",
    "publish:npm-component-prism": "cd packages/taro-fast-component-prism && npm publish",
    "publish:npm-framework": "cd packages/taro-fast-framework && npm publish",
    "reinstall": "npm run lerna:bootstrap",
    "show:all": "npx lerna ls -a -l",
    "show:info": "echo node version && node --version && echo npm version && npm --version && echo ------------ && npx lerna ls -a -l",
    "sleep:change-nrm": "node ./develop/assists/sleep.js",
    "start": "npm run start:simple:weapp",
    "start:simple:h5": "cd packages/simple-taro-fast-framework && npm run dev:h5",
    "start:simple:weapp": "cd packages/simple-taro-fast-framework && npm run dev:weapp",
    "update:cz:flag": "node ./develop/assists/cz.flag.refresh.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.13",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/config-lerna-scopes": "^17.4.2",
    "@commitlint/cz-commitlint": "^17.4.2",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-buble": "^1.0.2",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-url": "^8.0.1",
    "@svgr/rollup": "^6.5.1",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.3.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cssnano": "^5.1.14",
    "dayjs": "^1.11.7",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-taro": "^3.6.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.3",
    "lerna": "^6.4.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3",
    "prettier-plugin-packagejson": "^2",
    "rimraf": "^4.1.2",
    "rollup": "^3.12.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "shelljs": "^0.8.5",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0"
  }
}
