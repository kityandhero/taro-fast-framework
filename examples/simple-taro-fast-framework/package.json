    "docdash": "^2.0.2",
    "documentation": "^14.0.2",
    "easy-soft-develop": "^2.1.231",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
