{
  "name": "taro-fast-dva",
  "version": "2.9.0",
  "description": "The core lightweight library for dva, based on redux and redux-saga.",
  "license": "MIT",
  "sideEffects": false,
  "main": "/src/index.ts",
  "directories": {
    "es": "es"
  },
  "files": [
    "es"
  ],
  "scripts": {
    "build": "rimraf ./es && rollup --config rollup.config.compression.js --bundleConfigAsCjs",
    "build:skipCompression": "rimraf ./es && rollup --config rollup.config.skipCompression.js --bundleConfigAsCjs",
    "clean-file": "rimraf ./node_modules",
    "dev": "rollup --config rollup.config.skipCompression.js --bundleConfigAsCjs --watch",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "npx eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "npx eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "npx eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "npx prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "npx stylelint --fix \"./src/**/*.less\"",
    "precommit": "lint-staged",
    "prettier": "npx prettier -c --write \"**/*\"",
    "publish:compression": "npm run build && yarn publish",
    "v:p": "npm version patch && yarn publish"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.4",
    "@reduxjs/toolkit": "^1.8.6",
    "flatten": "^1.0.3",
    "global": "^4.4.0",
    "invariant": "^2.2.4",
    "is-plain-object": "^5.0.0",
    "redux-saga": "^1.2.1",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-external-helpers": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.19.4",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-babel": "6.0.2",
    "@rollup/plugin-buble": "^1.0.1",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-url": "^8.0.1",
    "@svgr/rollup": "^6.5.0",
    "@tarojs/mini-runner": "^3.5.7",
    "mm": "^3.2.0",
    "redux": "^4.2.0",
    "rollup": "^3.2.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1"
  },
  "peerDependencies": {
    "redux": "4.x"
  },
  "gitHead": "6bfee3113ac3fb9ab02bfcb26319b5aba42703f3"
}
