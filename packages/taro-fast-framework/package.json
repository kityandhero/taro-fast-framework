    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "taro-fast-common": "^1.0.75",
    "taro-fast-component": "^2.0.141"
  },
