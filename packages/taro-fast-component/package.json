  "main": "/src/index.ts",
  "scripts": {
    "publish:compression": "npm run build:rollup-compression && yarn publish",
    "v:p": "npm version patch && yarn publish",
    "clean-file": "rimraf ./node_modules",
    "build:rollup-noCompression": "rimraf ./es && rollup --config rollup.config.noCompression.js",
    "build:rollup-compression": "rimraf ./es && rollup --config rollup.config.compression.js",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "precommit": "lint-staged",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"./src/**/*.less\"",
    "prettier": "prettier -c --write \"**/*\""
  },
  "other": {},
